{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"rootDir": "src",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true
	},
	"include": ["src/**/*.ts"]
}
